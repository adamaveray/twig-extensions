{
  "name": "averay/twig-extensions",
  "type": "library",
  "license": "MIT",
  "version": "0.2.0",
  "homepage": "https://github.com/adamaveray/twig-extensions",
  "authors": [
    {
      "name": "Adam Averay",
      "homepage": "https://adamaveray.au/"
    }
  ],
  "autoload": {
